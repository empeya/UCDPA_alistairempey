   "source": [
    "# Lambda function to apply a column to the df returning 'Yes' if the daily change in oil is high and 'No' if not - volatility indicator\n",
    "df3['high_vol_oil'] = df3.apply(lambda x: 'Yes' if (x['% change_oil'] < -0.025 or x['% change_oil'] > 0.025) else 'No', axis=1)\n",
    "df3"
   ]
  },
